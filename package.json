{
  "name": "service-checker",
  "version": "0.6.5",
  "description": "Checks a variety of common web services for errors.",
  "repository": "mrkmg/service-checker",
  "bugs": {
    "url": "https://github.com/mrkmg/service-checker/issues"
  },
  "main": "main/index.js",
  "scripts": {
    "compile": "coffee -c -o main/ src/",
    "compile-watch": "coffee -cw -o main/ src/",
    "test": "npm run lint && npm run unit",
    "lint": "coffeelint -f .coffeelint.json src",
    "unit": "mocha --recursive ./main/test/specs --timeout 10000",
    "cover": "npm run env && npm run cover-local && npm run coveralls",
    "cover-local": "istanbul cover _mocha -- --timeout 10000 --recursive ./main/test/specs",
    "coveralls": "istanbul-coveralls"
  },
  "keywords": [
    "service",
    "checker"
  ],
  "author": "Kevin Gravier",
  "tonicExampleFilename": "example.js",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.0",
    "bluebird": "^3.0.5",
    "native-dns": "^0.7.0",
    "smtp-connection": "^1.3.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.1",
    "istanbul": "^0.4.1",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^2.3.4",
    "smtp-server": "^1.7.1"
  }
}