{
  "name": "service-checker",
  "version": "0.6.4",
  "description": "Checks a variety of common web services for errors.",
  "repository": "https://github.com/mrkmg/service-checker.git",
  "main": "index.js",
  "scripts": {
    "compile": "npm run compile-lib && npm run compile-test",
    "compile-lib": "coffee -c -o lib/ src/",
    "compile-test": "coffee -c -o test/specs/ test/specs-src/",
    "compile-watch": "npm-run-all --parallel compile-lib-watch compile-test-watch",
    "compile-lib-watch": "coffee -cw -o lib/ src/",
    "compile-test-watch": "coffee -cw -o test/specs/ test/specs-src",
    "test": "npm run lint && npm run unit",
    "lint": "coffeelint -f .coffeelint.json lib test/specs",
    "unit": "mocha --recursive test/specs --timeout 10000",
    "cover": "npm run env && npm run cover-local && npm run coveralls",
    "cover-local": "istanbul cover _mocha -- --timeout 10000 --recursive ./test/specs",
    "coveralls": "istanbul-coveralls"
  },
  "keywords": [
    "service",
    "checker"
  ],
  "author": "Kevin Gravier",
  "tonicExampleFilename": "example.js",
  "license": "MIT",
  "dependencies": {
    "async": "^1.5.0",
    "bluebird": "^3.0.5",
    "native-dns": "^0.7.0",
    "smtp-connection": "^1.3.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.1",
    "istanbul": "^0.4.1",
    "istanbul-coveralls": "^1.0.3",
    "mocha": "^2.3.4",
    "npm-run-all": "^1.3.3",
    "smtp-server": "^1.7.1"
  }
}